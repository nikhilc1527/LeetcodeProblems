#include <vector>
#include <string>

class Solution {
public:
  std::vector<std::string> generateParenthesis(int n) {
        if (n == 1) return {"()"};
        else if (n == 2) return {"()()", "(())"};
        else {
	  std::vector<std::string> prev = generateParenthesis(n - 1);
	  std::vector<std::string> ans;
	  for (std::string& str : prev) {
                ans.push_back("()" + str);
                ans.push_back("(" + str + ")");
                ans.push_back(str + "()");
            }
            return ans;
        }
    }
};
