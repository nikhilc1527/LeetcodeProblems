#include <iostream>

class Solution {
public:
  unsigned int reverseBits(unsigned int n) {
    unsigned int res = 0;
        for (int i = 0; i < 32; i++) {
            res <<= 1;
            res += (n & 1);
	    std::cout << (n & 0x1) << std::endl;
            n <<= 1;
        }
        return res;
    }
};
