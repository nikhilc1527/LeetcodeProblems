#include <vector>

class Solution {
  using namespace std;
  double average(vector<int>& salary) {
        int min = salary[0], max = salary[salary.size() - 1];
        double sum = 0;
        for (int i = 0; i < salary.size(); i++) {
            if (salary[i] > max) {
                sum += max;
                max = salary[i];
            }
        }
    }
};
