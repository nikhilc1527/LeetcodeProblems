#include <vector>
#include <string>

using namespace std;

class Solution {
public:
  int numIdenticalPairs(vector<int>& nums) {
    int res = 0;
    for (int i = 0; i < nums.size(); i++) {
      for (int j = i + 1; j < nums.size(); j++) {
	if (nums[i] == nums[j]) res++;
      }
    }

    return res;
  }

  int numSub(string s) {
    int res = 0;
    for (int i = 0; i < s.size(); i++) {
      for (int j = i; j < s.size(); j++) {
	if (s[i] == '1')
	  res++;
	else
	  break;
      }
    }

    return res;
  }
};
